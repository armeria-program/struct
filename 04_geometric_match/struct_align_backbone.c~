# include "struct.c"

/**************************************************************/
/*  postprocess - find the actual tf                          */
/* 	   & mapping on the bb level                          */
/**************************************************************/

int align_backbone (Descr *tgt_descr, Protein *tgt_structure,
		    Descr *qry_descr, Protein *qry_structure, 
		    List_of_maps *list, Score *score) {
    
    /* for now,  we will just postprocess the  best map */
    int best_ctr = 0;
    int map_ctr = list->map_best[best_ctr];
	
    postprocess (tgt_descr, tgt_structure, qry_descr, qry_structure, 
		 list->map+map_ctr, score);
    
    //rec_map_out_for_postproc (map, map_ctr, 0); 

 
    return 0;
	    
}
